Content-Type: text/plain; charset="UTF-8"

clc
x = [1 2 3 4];
disp('x = ')
disp(x)
Xk = fft(x);
disp('Xk = ')
disp(Xk)
Xmag = abs(Xk);
disp('Magnitude = ')
disp(Xmag)
Xang = angle(Xk);
disp('Phase = ')
disp(Xang)
n = 0:0.01:1;
subplot(4,1,1)
stem(x,LineWidth = 1.5,color = 'black')
xlabel('n')
ylabel('Amplitude')
title('Input Sequence')
grid on
subplot(4,1,2)
stem(real(Xk),LineWidth = 1.5,color = 'black')
xlabel('n')
ylabel('Amplitude')
title('FFT of the Input Sequence')
grid on
subplot(4,1,3)
stem(Xmag,LineWidth = 1.5,color = 'black')
xlabel('n')
ylabel('Magnitude')
title('Magnitude Spectrum')
grid on
subplot(4,1,4)
stem(Xang,LineWidth = 1.5,color = 'black')
xlabel('n')
ylabel('Phase')
title('Phase Spectrum')
grid on

